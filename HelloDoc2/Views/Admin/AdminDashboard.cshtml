@model DAL.ViewModel.AdminDashboardViewModel


@{    Layout = "~/Views/Shared/_LayoutMainAdminDashboard.cshtml";}<div class="bg-body-tertiary vh-100 d-flex flex-column">    <div class="">        <div class="p-sm-2 p-md-0 mt-4 mt-sm-0">            <div class="tab-content" id="myTabContent">                <div class="tab-pane fade show active" id="home-tab-pane" role="tabpanel" aria-labelledby="home-tab" tabindex="0">                    <div class="container-fluid">                        <div class="row">                            <div class="col-xl-2 col-lg-2 col-md-4 col-sm-4 col-4 mb-3">                                <div onclick="fetchRequests(1, null ,0)" class="border rounded p-1 p-sm-3 new" id="new-tab">
                                    <div class="d-flex align-items-center gap-1 ps-1 ms-3 mt-2">
                                        <i class="bi bi-person-plus d-none d-md-block fs-4"></i>                                        <h6 class="px-1 m-0">New</h6>                                    </div>
                                    <div class="ps-1 mt-3 ms-3 mb-3">
                                        <h4>@ViewBag.newRequest</h4>
                                    </div>                                </div>                            </div>                            <div class="col-xl-2 col-lg-2 col-md-4 col-sm-4 mb-3 col-4">                                <div onclick="fetchRequests(2, null ,0)" class="border rounded p-1 p-sm-3 Pending" id="Pending-tab">
                                    <div class="d-flex align-items-center gap-1 ps-1 ms-3 mt-2">
                                        <i class="bi bi-person me-2 d-none px-1 d-md-block fs-4"></i>                                        <h6 class="px-1 m-0">Pending</h6>                                    </div>
                                    <div class="ps-1 mt-3 ms-3 mb-3">
                                        <h4>@ViewBag.pendingRequest</h4>
                                    </div>                                </div>                            </div>                            <div class="col-xl-2 col-lg-2 col-md-4 col-sm-4 mb-3 col-4">                                <div onclick="fetchRequests(3,null , 0)" class="border rounded p-1 p-sm-3 Active" id="Active-tab">
                                    <div class="d-flex align-items-center gap-1 ps-1 ms-3 mt-2">
                                        <i class="bi bi-check2-circle me-2 d-none px-1 d-md-block fs-4"></i>                                        <h6 class="px-1 m-0">Active</h6>                                    </div>
                                    <div class="ps-1 mt-3 ms-3 mb-3">
                                        <h4>@ViewBag.activeRequest</h4>                                    </div>                                </div>                            </div>                            <div class="col-xl-2 col-lg-2 col-md-4 col-sm-4 mb-3 col-4">                                <div onclick="fetchRequests(4, null, 0)" class="border rounded p-1 p-sm-3 Conclude" id="Conclude-tab">
                                    <div class="d-flex align-items-center gap-1 ps-1 ms-3 mt-2">
                                        <i class="bi bi-clock-history me-2 d-none px-1 d-md-block fs-4"></i>                                        <h6 class="px-1 m-0">Conclude</h6>                                    </div>
                                    <div class="ps-1 mt-3 ms-3 mb-3">
                                        <h4>@ViewBag.concludeRequest</h4>                                    </div>                                </div>                            </div>                            <div class="col-xl-2 col-lg-2 col-md-4 col-sm-4 mb-3 col-4">                                <div onclick="fetchRequests(5, null ,0)" class="border rounded p-1 p-sm-3 ToClose" id="ToClose-tab">
                                    <div class="d-flex align-items-center gap-1 ps-1 ms-3 mt-2">
                                        <i class="bi bi-person-x me-2 d-none px-1 d-md-block fs-4"></i>                                        <h6 class="px-1 m-0">To Close</h6>                                    </div>
                                    <div class="ps-1 mt-3 ms-3 mb-3">
                                        <h4>@ViewBag.toCloseRequest</h4>                                    </div>                                </div>                            </div>                            <div class="col-xl-2 col-lg-2 col-md-4 col-sm-4 mb-3 col-4">                                <div onclick="fetchRequests(6, null ,0)" class="border rounded p-1 p-sm-3 Unpaid" id="Unpaid-tab">
                                    <div class="d-flex align-items-center gap-1 ps-1 ms-3 mt-2">
                                        <i class="bi bi-currency-dollar me-2 d-none px-1 d-md-block fs-4"></i>                                        <h6 class="px-1 m-0">Unpaid</h6>                                    </div>
                                    <div class="ps-1 mt-3 ms-3 mb-3">
                                        <h4>@ViewBag.unpaidRequest</h4>                                    </div>                                </div>                            </div>                        </div>                    </div>                        <div id="partial_container">                            <partial name="_RequestsAccToStatus" />                        </div>                    </div>                </div>                <div class="tab-pane fade" id="provider-location-tab-pane" role="tabpanel" aria-labelledby="provider-location-tab" tabindex="0">                    <div class="container-fluid">                        <h3 class="mb-3 p-3 p-md-2 ">Provider Location</h3>                    </div>                </div>                <div class="tab-pane fade" id="profile-tab-pane" role="tabpanel" aria-labelledby="profile-tab" tabindex="0">                    <div class="container-fluid">                        <h3 class="mb-3 p-3 p-md-2 ">My Profile</h3>                    </div>                </div>                <div class="tab-pane fade" id="Providers-tab-pane" role="tabpanel" aria-labelledby="Providers-tab" tabindex="0">                    <div class="container-fluid">                        <h3 class="mb-3 p-3 p-md-2 ">Providers</h3>                    </div>                </div>                <div class="tab-pane fade" id="Partners-tab-pane" role="tabpanel" aria-labelledby="Partners-tab" tabindex="0">                    <div class="container-fluid">                        <h3 class="mb-3 p-3 p-md-2 ">Partners</h3>                    </div>                </div>                <div class="tab-pane fade" id="Access-tab-pane" role="tabpanel" aria-labelledby="Access-tab" tabindex="0">                    <div class="container-fluid">                        <h3 class="mb-3 p-3 p-md-2 ">Access</h3>                    </div>                </div>                <div class="tab-pane fade" id="Records-tab-pane" role="tabpanel" aria-labelledby="Records-tab" tabindex="0">                    <div class="container-fluid">                        <h3 class="mb-3 p-3 p-md-2 ">Records</h3>                    </div>                </div>            </div>        </div>    </div></div>        <footer class="bg-dark container-fluid fixed-bottom">
        <div class="footer d-flex justify-content-end">
            <div class="m-2">
                <a class="text-white">Terms of Conditions</a> <span style="color: gray;">|</span> <a class="text-white">Privacy</a>
            </div>
        </div>
    </footer><script>    function fetchRequests(status, reqtypeid,regionid) {        console.log("inside fetchreq");        var status = status;        var reqtypeid = reqtypeid;        var regionid = regionid;        $.ajax({            method: "GET",            url: "/Admin/fetchRequests",            data: { status: status,                reqtypeid: reqtypeid,                regionid : regionid               },            success: function (response) {                console.log("Function Success")                $('#partial_container').html(response)            },            error: function () {                console.log("Function Fail")            }        })    }    function showmodel() {        var myModal = new bootstrap.Modal(document.getElementById('myModal1'), {})        myModal.show()    }</script>@* <script src="https://cdn.datatables.net/2.0.0/js/dataTables.js"></script> *@