@{
    Layout = "/Views/Shared/_LayoutNone.cshtml";
}
<head>
    <link rel="stylesheet" href="~/css/AdminDashboard.css" />
</head><div class="container-fluid bg-bodybg-body-tertiary vh-100">    <partial name="_LayoutAdminDashboard" />    <div class="container d-flex flex-column gap-2 p-3 ">        @*Header of view page*@        <div class="container d-flex justify-content-between p-0 my-2">            <div>                <h5>                    View Reservation                    <span class="badge bg-success p-2 bg-gradient border rounded-5" style="--bs-bg-opacity: .9; font-size: 0.8rem; padding: 8px 12px;">                        Patient                    </span>                </h5>            </div>            <a asp-controller="Admin" asp-action="AdminDashboard">                <span class="border border-info text-info p-2 rounded-2">                    < Back                </span>            </a>        </div>        @*FORM*@        <form method="post" enctype="multipart/form-data" class="container d-flex flex-column shadow gap-4 p-4">            <div class="container">                <h4 class="ms-auto me-auto mb-4">Patient Information</h4>                <h6 style="font-size: 14px">Confirmation Number</h6>                <p class="text-info fs-5 mb-3">MD092723TETE0002</p>                <div>                    <div class="form-floating mb-3 h-100">                        <textarea class="form-control bg-transparent h-100" id="floatingInput" placeholder="Textarea" rows="3"></textarea>                        <label for="floatingInput" class="text-wrap ">Patient Notes</label>                    </div>                    <div class="mt-1">                        <div class="row">                            <div class="form-floating mb-3 col-12 col-sm-6">                                <input type="text" class="form-control bg-transparent" id="floatingInput" placeholder="John" required >                                <label for="floatingInput" class="ms-2 ">First Name</label>                            </div>                            <div class="form-floating mb-3 col-12 col-sm-6">                                <input type="text" class="form-control bg-transparent" id="floatingInput" placeholder="Wick" >                                <label for="floatingInput" class="ms-2 ">Last Name</label>                            </div>                            <div class="form-floating mb-3 col-12 col-sm-6">                                <input type="date" class="form-control bg-transparent" id="floatingInput" placeholder="02-10-2002" >                                <label for="floatingInput" class="ms-2 ">Date of Birth</label>                            </div>                            <div class="form-floating mb-3 col-12 col-sm-5">                                <input type="tel" class="phone form-control bg-transparent" id="floatingInput" placeholder="1234567890" >                            </div>                            <div class="form-floating mb-3 col-12 col-sm-1 ">                                <i class="btn bi bi-telephone border border-info rounded-2 px-4 py-3 text-info"></i>                            </div>                        </div>                    </div>                    <div class="row">                        <div class="form-floating mb-3 col-12 col-sm-6">                            <input type="email" class="form-control bg-transparent emailInp" id="floatingInput1" placeholder="test@google.com" >                            <label for="floatingInput" class="ms-2 ">Email</label>                        </div>                    </div>                    <div class="row">                        <h4 class="ms-auto me-auto">Location Information</h4>                        <div class="form-floating mb-3 col-12 col-sm-6">                            <input type="text" class="form-control bg-transparent" id="floatingInput" placeholder="Maryland" >                            <label for="floatingInput" class="ms-2 ">Region</label>                        </div>                        <div class="form-floating mb-3 col-12 col-sm-5">                            <input type="text" class="form-control bg-transparent" id="floatingInput" placeholder="Maryland" >                            <label for="floatingInput" class="ms-2 ">Business Name / Address</label>                        </div>                        <div class="form-floating mb-3 col-12 col-sm-1 ">                            <i class="btn bi bi-geo-alt border border-info rounded-2 text-info px-4 py-3 text-info"></i>                        </div>                        <div class="form-floating mb-3 col-12 col-sm-6">                            <input type="text" class="form-control bg-transparent" id="floatingInput" placeholder="007" >                            <label for="floatingInput" class="ms-2 ">Room #</label>                        </div>                    </div>                    <div class="d-flex justify-content-end mb-3">                        <div class="d-flex">                            <div class="form-floating mx-2">                                <button type="button" class="btn text-white btn-info">Assign</button>                            </div>                            <div class="form-floating mx-2">                                <button type="button" class="btn text-white btn-info">View Notes</button>                            </div>                            <div class="form-floating mx-2">                                <button type="button" class="btn text-white btn-danger">Cancel</button>                            </div>                        </div>                    </div>                </div>        </form>    </div></div>